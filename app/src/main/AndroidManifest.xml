<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sallyjayz.contactlistwithroomdatabase">

    <application
        android:name=".ContactsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ContactListWithRoomDatabase">
        <activity android:name=".user.SignupActivity"
            android:label="@string/sign_up">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.LoginActivity"
            android:label="@string/login"/>
        <activity android:name=".contact.AddContactActivity"
            android:label="@string/add_contact"/>
        <activity android:name=".contact.ContactListActivity"
            android:label="@string/contacts"/>
    </application>

</manifest>